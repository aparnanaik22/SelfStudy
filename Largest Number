#include <iostream>
using namespace std;

int main() 
{
	int a,b,c;
	int max=0;
	cin>>a>>b>>c;
	cout<<"Enter any three numbers:"<<a<<","<<b<<","<<c<<endl;
	{
		if(a>b)
			max=a;
		else
			max=b;
	}
	{	
		if(max>c)
			max=max;
		else
			max=c;
	}
	cout<<"The largest number is: "<<max<<"\n";
	return 0;
}
