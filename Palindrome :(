#include <iostream>
using namespace std;

int main() 
{
	int n,dn,temp=0,d;
	cin>>n;
	cout<<"Enter a number="<<n<<"\n";
	dn=n;
	while(dn!=0)
	{
		d=dn%10;
		temp=(temp*10)+d;
		dn=dn/10;
	}
	if(n==temp)
	cout<<"The given number is a palindrome.";
	else
	cout<<"The given number is not a palindrome.";
	return 0;
}
